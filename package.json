{
  "name": "@nyffels/html-to-pdf",
  "description": "A docker server with Swagger API implemented to convert HTML code to PDF document.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "start": "node dist/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nyffels-IT/HTML-to-PDF-Docker-Swagger-API",
    "homepage": "https://github.com/Nyffels-IT/HTML-to-PDF-Docker-Swagger-API"
  },
  "bugs": {
    "url": "https://github.com/RyanTheAllmighty/Docker-Hub-API/issues",
    "email": "chesney@nyffels.be"
  },
  "keywords": [],
  "author": {
    "name": "Chesney Nyffels",
    "email": "chesney@nyffels.be",
    "url": "http://nyffels.be"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.13",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@types/joi": "^17.2.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "puppeteer": "^22.10.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.2.1"
  }
}
